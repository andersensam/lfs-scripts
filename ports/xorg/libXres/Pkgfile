# Description: Xorg Library for libXres
# URL:         
# Maintainer:  LFS
# Depends on:  fontconfig libxcb

name=libXres
version=1.2.0
release=1
source=(https://www.x.org/pub/individual/lib/libXres-1.2.0.tar.bz2)

build() {
        cd $name-$version

        source /etc/profile.d/xorg.sh

        ./configure $XORG_CONFIG $docdir

        make
        make DESTDIR=$PKG install
}
